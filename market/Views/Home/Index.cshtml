@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Home</title>
</head>
<body>
    @foreach (Clothes item in Model)
    {
        <div>
            <a asp-action="ShowItem" asp-route-id="@item.id">@item.name</a>
            <p>@item.price $</p>
        </div>
        <br />
    }
</body>
</html>

